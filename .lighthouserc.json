{
    "ci": {
      "collect": {
      "numberOfRuns": 3,
      "staticDistDir": "dist/superheroes-app/browser"
    },
    "assert": {
        "preset": "lighthouse:all",
        "assertions": {
          "aria-text": "warn",
          "bf-cache": "warn",
          "bootup-time": "warn",
          "canonical": "warn",
          "csp-xss": "warn",
          "cumulative-layout-shift": "warn",
          "dom-size": "warn",
          "duplicated-javascript": "warn",
          "errors-in-console": "warn",
          "first-contentful-paint": "warn",
          "first-meaningful-paint": "warn",
          "font-size": "warn",
          "heading-order": "warn",
          "inspector-issues": "warn",
          "interactive": "warn",
          "largest-contentful-paint": "warn",
          "long-tasks": "warn",
          "mainthread-work-breakdown": "warn",
          "max-potential-fid": "warn",
          "non-composited-animations": "warn",
          "prioritize-lcp-image": "warn",
          "render-blocking-resources": "warn",
          "server-response-time": "warn",
          "service-worker": "warn",
          "speed-index": "warn",
          "tap-targets": "warn",
          "total-byte-weight": "warn",
          "unsized-images": "warn",
          "unused-css-rules": "warn",
          "unused-javascript": "warn",
          "uses-long-cache-ttl": "warn",
          "uses-rel-preconnect": "warn"
        }
    },
      "upload": {
        "target": "temporary-public-storage"
      }
    }
  }
  